{
  "manifest_version": 3,
  "name": "chrome-extension-boilerplate",
  "version": "0.1.0",
  "description": "Chrome插件模板",
  "background": {
    "service_worker": "src/pages/background/index.js"
  },
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/pages/demoContent/index.js"
      ]
    },
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "src/pages/demo2Content/index.js"
      ]
    }
  ]
}