{
  "manifest_version": 3,
  "name": "tuia-chrome-toolkit",
  "version": "0.1.0",
  "description": "推啊Chrome工具",
  "background": {
    "service_worker": "src/pages/background/index.js"
  },
  "permissions": [
    "cookies"
  ],
  "host_permissions": [
    "*://*.tuiatest.cn/*",
    "*://*.tuia.cn/*",
    "*://*.7hotest.com/*"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.tuiatest.cn/*",
        "*://*.tuia.cn/*",
        "*://*.7hotest.com/*",
        "http://localhost:3010/*",
        "http://172.16.224.246:3010/*"
      ],
      "js": [
        "src/pages/sceneContent/index.js"
      ]
    },
    {
      "matches": [
        "*://activity.tuiatest.cn/*",
        "*://activity.tuia.cn/*",
        "*://activity.tuifish.com/*"
      ],
      "js": [
        "src/pages/deviceContent/index.js"
      ]
    }
  ]
}